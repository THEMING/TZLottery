@CHARSET "UTF-8";

ul, ol { list-style: none; }

ul li {
	margin:0px;
	float:left;
}

.left {
	text-align:left;
}

.right {
	text-align:right;
}

/**********框架**************/
.lottery_main {
	width: 986px;
	margin-top: 0px;
	margin-right: auto;
	margin-bottom: 0;
	margin-left: auto;
}

.left_box {
	width: 700px;
	float:left;
}

.right_box {
	width: 250px;
	margin-left: 10px;
	border: thin solid #FBCCA5;
	border-bottom: none;
	float:left;
}

.left_box .left_top_box {
	height: 90px;
	border: thin solid #FBCCA5;
}

.left_box .left_bottom_box {
	margin-top: 10px;
	text-align: center;
}	

/*************切换框*****************/
.lot_box {
	float: left; 
	margin: 0px; 
	width: 678px;
	display: inline;
	margin-left:10px;
}

.lot_box .lot_top ul li{
	float: left;
	width: 68px;
	height: 26px;
	margin-right: 2px;
	text-align: center;
	line-height: 26px;
	color: #7F2D00;
	cursor: pointer;
	position: relative;
	top: 1px;
}

.lot_box .lot_top ul li.on {
	font-weight: bold; 
	background-image: url(../images/common/buy-titile.png);
	height: 27px; 
}

.lot_box .lot_main {
	border: 1px #FFCCFF solid;
	padding-left: 5px;
}

.lot_box .lot_main ul {
	 padding: 0px; 
	 margin: 10px;
}

/***************球的状态*******************/

.red_ball_true,
.blue_ball_false, 
.blue_ball_true, 
.red_ball_false, 
.org_ball {
	width:35px;
	height:35px;
	text-align:center;
	font:14px/35px Arial;
	cursor:pointer;
	color:#fff;
	background:url(../images/lottery/ball.png) no-repeat 0 0;
}

.blue_ball_true {background-position:0 -91px;}

.red_ball_false, 
.blue_ball_false {
	color:#757575;
	background-position:0 -46px;
}

.org_ball {
	background-position:0 -160px;
}

.lottery_list {
	width:450px;
	height:90px;
	overflow-y:scroll;
	padding:2px;
	background: #fffffe url(../images/text_bg.png) repeat-x bottom fixed;
	border:1px solid #aeb1b6;
	margin-bottom:6px;
	float: left;
}

.betfont_red{color:#f00;}
.betfont_blue{color:#2073fc;}

.betfont_black{
	background-color:#FFFFFF;
	color:#0099FF;
}

.lottery_list ul li {
		
	line-height:20px;
	border-bottom:1px dashed #c2c2c2;
}

.lottery_list ul li span {
	margin: 0 10px;
	cursor: pointer;
}

.ssq_bet_list_box {
	height: 230px;
	border: thin solid #99CCFF;
}

.ssq_bet_list_box .left {
	text-align:center; 
	float:left;
	width:80px;
	height:20px;
	background:#EEEEEE;
	padding-bottom:105px;
	padding-top:105px;
}

.ssq_bet_list_box .list_box {
	float: left;
	width: 612px;
}

.uploadfile_box {
	
}

.select-list {
	height:132px;
	width: 450px;
}

.lot_bet_type_box {
	height: 220px;
	border: thin solid #99CCFF;
	background-color:#FFFFFF;
}

.lauch_table {
	width: auto;
	height: auto;
	margin: 5px;
	float: left;
}

.lauch_table td {
	border-bottom: 1px #FFCCFF dashed;
}

.lauch_table .right {
	text-align: right;
}

.chase_table_box {
	float: left;
	width: 550px;
	height:145px;
	overflow-y:scroll;
	padding:5px;
	border:1px solid #aeb1b6;
	margin-bottom:10px;
}


.common_box .box_top h1{float: left;}
.common_box .box_top .more{float: right;}
.common_box .box_main {border-top: 1px solid #FBCCA5; border-bottom: 1px solid #FBCCA5; padding: 5px;}

.common_box .box_top {
	height: 27px;
	padding: 0 10px;
	line-height: 27px;
	background:url(../images/369caibg/369cai_14.gif) repeat-x;
}

.common_box .box_main .soft_list li {
	line-height: 25px; 
	float: left; 
	width: 87px;
}

.common_box .box_main .soft_list li a{color: #333333;}
.common_box .box_main .soft_list li a:hover{color: #FF0000; text-decoration: none;}

.common_box .kjHistory_li1 {
	width:45px;
	text-align: center;
}

.common_box .kjHistory_li2 {
	width:70px;
	text-align: center;
}

.common_box .kjHistory_li3 {
	width:100px;
	text-align: center;
}

/****** 剩余时间 **************/

.remainTime_box {
	margin-top:10px;
	background:#000000;
	color:#00FF00;
	font-size:14px;
	text-align:center;
}

.remainTime_box span {
	color:#00FF00;	
}

/****** 玩法选择 **************/

.playAndBuy_box_top {
	background:url(../images/369caibg/369cai_14.gif) repeat-x;
}

/****** 上传文件 **************/
.upload_box {
	height: 220px;
	border: thin solid #99CCFF;
}

.upload_box .left {
	text-align:center; 
	float:left;
	width:80px;
	height:20px;
	background:#EEEEEE;
	padding-bottom:100px;
	padding-top:100px;
}

