.talkarea {
	width:906px;
	height:180px;
    border:1px solid #f90;
	border-top:0px;
}
.talkarea .fabiaokuang {
	float:left;
	width:404px;
	height:100px;
	margin-top:15px;
	margin-left:20px;
}
.talkarea .fabiaokuang .textarea {
	width:300px;
	height:40px;
	border:2px solid #f60;
	overflow:scroll right;
}
.talkarea .fabiaokuang .BTN-to-SHOW {
	float:right;
	width:60px;
	margin-top:10px;
	border:2px solid #f60;
	background:#fff;
}
.screen-talk {
	float:left;
	margin-top:10px;
	margin-left:50px;
	width:350px;
	height:150px;
	overflow:scroll;
    -webkit-box-shadow: 0px 0px 9px #666;
	-moz-box-shadow: 0px 0px 8px #666;
	box-shadow: 0px 0px 8px #666;
	border:1px solid #999;
}
.screen-talk ul li {
	margin:0 auto;
	margin-top:8px;
	width:320px;
	height:auto;
	border-bottom:1px dotted #999;
	color:#000;
	line-height:1.6;
}

#gallery {font:13px verdana,arial,sans-serif; width:750px; padding:2px 0 0 0; line-height:2;}
#gallery li.off {color:#000; height:24px; margin-right:2px; line-height:33px; padding:0 20px; float:left; background:url(../images/tabs_0.gif) repeat-x left bottom; border:1px solid #ddd; border-bottom-color:#fc9; cursor:pointer; position:relative; z-index:20;}
#gallery li.on {color:#c00; padding:0 20px; margin-right:2px; margin-top:1px; float:left; background:url(../images/tabs_2.gif) repeat-x left bottom; border:1px solid #Fc9; cursor:pointer; border-bottom:0; height:24px; position:relative; z-index:100;}
#gallery1 {font:11px verdana,arial,sans-serif; width:750px; padding:2px 0 0 0; line-height:2;}
#gallery1 li.off {color:#000; height:24px; margin-right:2px; line-height:33px; padding:0 20px; float:left; background:url(../images/tabs_0.gif) repeat-x left bottom; border:1px solid #ddd; border-bottom-color:#fc9; cursor:pointer; position:relative; z-index:20;}
#gallery1 li.on {color:#c00; padding:0 20px; margin-right:2px; margin-top:1px; float:left; background:url(../images/tabs_2.gif) repeat-x left bottom; border:1px solid #Fc9; cursor:pointer; border-bottom:0; height:24px; position:relative; z-index:100;}

li.hide {display:none; width:0; overflow:hidden;}
li.show {clear:left; background:#fff; width:708px; margin-top:0; top:-1px; border:1px solid #c9; position:relative; z-index:50; font:11px verdana, arial, sans-serif; line-height:18px;}
li.show img {float:left; margin:0 10px 10px 0;}