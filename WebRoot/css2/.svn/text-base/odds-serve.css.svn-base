@charset "utf-8";


.content-box {
	width:1000px;
	height:auto;
	margin:0 auto;
	margin-top:8px;
}
.left-list {
	float:left;
	width:180px;
}
dl.leftyuanjiao {
	background:#ff6000 url(../images/369caibg/gg_03.png) no-repeat scroll left top;
	width:180px;
	text-align:left;
}
dl.leftyuanjiao dt {
	background: url(../images/369caibg/gg_05.png) no-repeat scroll right top;
	padding:10px;
	text-align:left;
}

.cont-regis {
	width:158px;
	height:54px;
	border:1px solid #FC9;
	padding-left:20px;
	padding-top:16px;
}


dl.zc-listyuanjiao {
	background:#e59a52 url(../images/369caibg/bb_03.png) no-repeat scroll left top;
	width:180px;
	text-align:left;
	margin-top:10px;
	border-bottom:1px solid #FCC;
}
dl.zc-listyuanjiao dt {
	background: url(../images/369caibg/bb_05.png) no-repeat scroll right top;
	text-align:left;
	font-size:14px;
	padding-left:10px;
	line-height:2;
	font-weight:bold;
	color:white;	
}

.list-of-type {
	width:178px;
	height:auto;
	padding-bottom:10px;
	border-top:0px;
	border:1px solid #fc9;
}
.list-of-type ul .zcbiaoti {
	width:168px;
	background:#e59a52;
	text-align:left;
	margin-top:10px;
	border-bottom:1px solid #FCC;
	line-height:2;
	font-size:14px;
	font-weight:bold;
	color:white;
	padding-left:10px;
}
.list-of-type ul li .list-two {
	width:140px;
	height:110px;
	margin:0 auto;
    padding-left:10px;
	padding-bottom:10px;
	margin-top:8px;
	border-bottom:1px solid #fc9;
}
.list-of-type ul li .list-two li a {
	line-height:1.9;
	font:"Trebuchet MS", Arial, Helvetica, sans-serif;
	font-size:12px;
	color:#000;
}
.list-of-type ul li .list-two li a:hover {
	color:red;
}
