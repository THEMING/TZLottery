}
.selectduizheng {
	width:986px;
	height:28px;
	margin-top:10px;
}
.selectduizheng ul {
	float:left;
} 
.selectduizheng ul .left {
	float:left;
	height:28px;
	background: url(../images/left.png) no-repeat left;
	padding-left:5px;
}
.selectduizheng ul .right {
	float:left;
	height:28px;
	background:url(../images/right.png) no-repeat left;
	padding-left:6px;
} 
.selectduizheng ul .middle {
	float:left;
	width:968px;
	height:28px;
	background:url(../images/zijibg.png) repeat-x;
}
.selectduizheng ul .middle ul {
	display:block;
	width:968px;
	height:28px;
	float:left;
}
.selectduizheng ul .middle ul .tab {
	float:left;
	width:100px;
	height:24px;
	margin-top:3px;
	text-align:center;
	color:#000;
	border:1px solid #C99;
	border-bottom:0px;
}
.selectduizheng ul .middle ul .arrow {
	background:url(../images/369caibg/arrow.gif) right no-repeat;
	background-position:right -10px;
}
.tab-current {
	background:#fff;
	border:1px solid #F90;
}
.compare-table {
    width:976px;
	border:1px solid #f90;
