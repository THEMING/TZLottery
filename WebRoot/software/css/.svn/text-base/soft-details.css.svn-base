body,ul,ol,li,p,h1,h2,h3,h4,h5,h6,form,fieldset,table,td,img,div,th{
	border:0;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0; 	
	margin-left: 0px;
	padding: 0; 
	float: none;
	font-family:"풕턼환쥌";
	
}
.* { margin:0px; padding:0px;}
ul,ol{
	font-size: 12px;
	list-style-type: none;
