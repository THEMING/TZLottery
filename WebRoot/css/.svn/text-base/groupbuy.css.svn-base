.start-groupBUY {
	width:790px;
	height:auto;
	border:2px solid #F90;
	margin-bottom:10px;
	background:#F0F0EC;
	line-height:3;
	padding:20px 0px 20px 0px;
}
.start-groupBUY p {
	padding-left:30px;
}
.start-groupBUY span {
	width:180px;
	margin-left:10px;
	padding:2px 10px 2px 10px;
	line-height:27px;
	background:#390;
	font-size:14px;
	color:white;
}
#gallery a {
	padding:10px;
	display:inline-block;
	text-align:left;
	padding-left:30px;
	cursor:pointer;
}


#gallery a.on {
	width:129px;
	padding:5px 0px 5px 30px;
	margin-top:7px;
	font-size:15px;
	text-align:left;
	margin-left:20px;
	margin-bottom:3px;
	color: #f60;
	cursor:pointer;
	border:2px solid #ffcc7f;
	border-right:2px solid #fff;
}