package com.xsc.lottery.wzl.bean;

public class TwoThousandNineBean {
public String merchantid;
public String lotoid;
public String issue;
public String getLotoid() {
	return lotoid;
}
public void setLotoid(String lotoid) {
	this.lotoid = lotoid;
}
public String getIssue() {
	return issue;
}
public void setIssue(String issue) {
	this.issue = issue;
}
public String toXml(){
	String xml="<body><salecheck merchantid=\""+this.merchantid+"\"  lotoid=\""+this.lotoid+"\" issue=\""+this.issue+"\"/></body>";
	return xml;
}
public String getMerchantid() {
	return merchantid;
}
public void setMerchantid(String merchantid) {
	this.merchantid = merchantid;
}

}
