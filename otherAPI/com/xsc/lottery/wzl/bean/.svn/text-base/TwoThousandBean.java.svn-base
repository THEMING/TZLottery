package com.xsc.lottery.wzl.bean;

public class TwoThousandBean 
{
	public String lotoid;
	public String issue;

	public String getLotoid() {
		return lotoid;
	}

	public void setLotoid(String lotoid) {
		this.lotoid = lotoid;
	}

	public String getIssue() {
		return issue;
	}

	public void setIssue(String issue) {
		this.issue = issue;
	}

	public String toXml() {
		String xml = "<body><loto lotoid=\"" + this.lotoid + "\" issue=\""
				+ this.issue + "\"/></body>";
		return xml;
	}
}
